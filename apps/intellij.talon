os: linux
app: jetbrains-idea
-
hide project:
    key(shift-escape)
snip if none:    
    insert("ifn")
    key(tab)
snip if not none: 
    insert("ifnn")
    key(tab)

snip iterate array:
    insert("itar")
    key(tab)
snip iterate collection:
    insert("itco")
    key(tab)
snip print format:
    insert("souf")
    key(tab)
snip print line:
    insert("soutc")
    key(tab)
